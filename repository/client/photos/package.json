{
  "name": "app-photos",
  "version": "0.1.0",
  "private": true,
  "homepage": "/photos/",
  "dependencies": {
    "@material-ui/core": "~4.3.0",
    "@material-ui/icons": "~4.2.1",
    "@reactivex/rxjs": "~6.5.2",
    "ajv": "^6.7.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-grid-gallery": "^0.5.3",
    "react-intl": "^2.4.0",
    "react-router-dom": "~5.0.1",
    "react-scripts": "~3.0.1",
    "rxjs": "~6.5.2",
    "rxjs-compat": "^6.5.2",
    "superagent": "~5.1.0"
  },
  "scripts": {
    "start": "npm run copy:library && react-scripts start",
    "build": "npm run copy:library && set NODE_ENV=development && react-scripts build && npm run copy:build",
    "test": "react-scripts test --env=jsdom",
    "copy:library": "cp -rv ../_library/** src/library",
    "copy:build": "rm -fr ../../src/main/resources/static/photos/** && cp -rv build/** ../../src/main/resources/static/photos",
    "translate": "NODE_ENV=production babel ./src >/dev/null",
    "manage:translations": "node ./scripts/translationRunner.js",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@storybook/addon-actions": "~5.1.10",
    "@storybook/addon-links": "~5.1.10",
    "@storybook/addons": "~5.1.10",
    "@storybook/react": "~5.1.10",
    "aemsync": "~4.0.0",
    "babel-plugin-react-intl": "~4.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5"
  }
}
