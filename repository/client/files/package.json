  "scripts": {
    "start": "react-scripts start",
    "build": "set NODE_ENV=development && npm run copy:library && react-scripts build --profile && npm run copy:build",
    "test": "react-scripts test --env=jsdom",
    "copy:library": "cp -rv ../_library/** src/library",
    "copy:build": "rm -fr ../../src/main/resources/static/files/** && cp -rv build/** ../../src/main/resources/static/files && cp -rv build/** ../../target/classes/static/files",
    "translate": "NODE_ENV=production babel ./src >/dev/null",
    "manage:translations": "node ./scripts/translationRunner.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
