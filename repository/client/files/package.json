{
  "name": "files",
  "version": "0.2.0",
  "private": true,
  "homepage": "/files/",
  "scripts": {
    "start": "npm run copy:library && react-scripts start",
    "build": "set NODE_ENV=development && npm run copy:library && npm run i18nParser && react-scripts build --profile && npm run copy:build",
    "test": "react-scripts test --env=jsdom",
    "copy:library": "cp -rv ../_library/** src/library",
    "copy:build": "rm -fr ../../src/main/resources/public/files/** && cp -rv build/** ../../src/main/resources/public/files && cp -rv build/** ../../target/classes/public/files",
    "translate": "NODE_ENV=production babel ./src >/dev/null",
    "manage:translations": "node ./scripts/translationRunner.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "i18nParser": "i18next --config .i18next-parser.config.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}