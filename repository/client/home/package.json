{
  "name": "home",
  "version": "0.1.0",
  "private": true,
  "homepage": "/home",
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "@reactivex/rxjs": "^6.5.2",
    "babel-loader": "8.0.6",
    "flexboxgrid": "^6.3.1",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-css-grid-layout": "^1.0.0",
    "react-dom": "^16.5.2",
    "react-intl": "^2.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "superagent": "~5.1.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "babel-core": "^7.5.5",
    "babel-plugin-react-intl": "^4.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-1": "^6.24.1",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "react-intl-translations-manager": "^5.0.3"
  },
  "scripts": {
    "start": "npm run copy:library && react-scripts start",
    "build": "set NODE_ENV=development && npm run copy:library && react-scripts build && npm run copy:build",
    "test": "react-scripts test --env=jsdom",
    "copy:library": "cp -rv ../_library/** src/library",
    "copy:build": "rm -f ../../src/main/resources/static/home/*.* && rm -f ../../src/main/resources/static/home/static/**/*.* && cp -rv build/** ../../src/main/resources/static/home",
    "eject": "react-scripts eject",
    "translate": "NODE_ENV=development babel ./src >/dev/null",
    "manage:translations": "node ./scripts/translationRunner.js",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
