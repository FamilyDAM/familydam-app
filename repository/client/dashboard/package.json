    "react-dom": "^16.5.2",
    "react-intl": "^2.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.4",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.3.2",
    "superagent": "^3.8.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
